               <ref name="front"/>
            </optional>
            <oneOrMore>
               <choice>
                  <ref name="model.graphicLike"/>
                  <ref name="surface"/>
                  <ref name="surfaceGrp"/>
               </choice>
            </oneOrMore>
            <optional>
               <ref name="back"/>
